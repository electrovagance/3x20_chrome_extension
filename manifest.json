{
    "name": "3x20",
    "version": 1.0,
    "description": "Helps with reducing eyestrain by reminding you to take a break according to the 20-20-20 rule",
    "manifest_version": 2,
    "icons": {"128": "stopwatch-solid-fontawesome.png"},
    "browser_action": {
        "default_icon": "stopwatch-solid-small-fontawesome.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}